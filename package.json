{
  "name": "weggejorist",
  "version": "1.0.0",
  "description": "Shows all deleted Dumpert comments",
  "main": "index.js",
  "scripts": {
    "start": "DEBUG=weggejorist,weggejorist:* babel-watch src/index.js",
    "start-worker": "DEBUG=weggejorist,weggejorist:* babel-watch src/worker.js",
    "build": "babel src -d dist",
    "serve": "node --optimize_for_size --max_old_space_size=460 --gc_interval=100 dist/index.js",
    "serve-worker": "forever dist/worker.js",
    "test": "eslint \"src/**\""
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bluebird": "^3.4.7",
    "cors": "^2.8.1",
    "debug": "^2.6.0",
    "express": "^4.14.0",
    "forever": "^0.15.3",
    "mongoose": "^4.7.4",
    "request": "^2.79.0",
    "request-promise": "^4.1.1",
    "x-ray": "^2.3.1",
    "xml2js": "^0.4.17"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-preset-env": "^1.1.8",
    "babel-watch": "^2.0.5",
    "eslint": "^3.13.1"
  }
}
